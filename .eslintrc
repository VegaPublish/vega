{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "sanity",
    "sanity/react",
    "prettier",
    "prettier/react",
    "prettier/flowtype"
  ],
  "globals": {
    "__DEV__": true
  },
  "plugins": ["prettier", "flowtype"],
  "rules": {
    "flowtype/define-flow-type": 1,
    "operator-linebreak": "off",
    "newline-per-chained-call": 0,
    "sort-imports": 0,
    "prettier/prettier": "error",
    "complexity": "off",
    "react/prefer-stateless-function": "off"
  }
}
