{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "ie": "9"
        }
      }
    ]
  ],
  "plugins": [
    "lodash",
    "syntax-class-properties",
    "transform-class-properties",
    "syntax-object-rest-spread",
    "transform-object-rest-spread",
    "syntax-flow",
    "transform-flow-comments"
  ]
}
